<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>Barracuda.OpenApi</RootNamespace>
    <AssemblyName>Barracuda.OpenApi</AssemblyName>
    <Authors>Jesús Lopez, César Rodríguez</Authors>
    <Company>Jesús López</Company>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Product>Barracuda.OpenApi</Product>
    <PackageId>Barracuda.OpenApi</PackageId>
    <Version>1.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Templates\Auth.html" />
    <None Remove="Templates\OpenApi.html" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Templates\Auth.html" />
    <EmbeddedResource Include="Templates\OpenApi.html" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

</Project>
