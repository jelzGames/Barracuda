<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>Barracuda.OpenApi</RootNamespace>
    <AssemblyName>Barracuda.OpenApi</AssemblyName>
    <Authors>Jesús Lopez, César Rodríguez</Authors>
    <Company>Jesús López</Company>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Product>Barracuda.OpenApi</Product>
    <PackageId>Barracuda.OpenApi</PackageId>
    <Version>1.0.1</Version>
    <Description>Baracuda OpenAPI Azure Functions NET Core 3.1
Generate the json file openAPI 3.0 and the front end view to interact with the API’s calls based on the setting defined in the json file openAPI 3.0.</Description>
    <PackageProjectUrl>https://www.chambapatodos.com/</PackageProjectUrl>
    <PackageIcon>barracuda 150x150 (1).png</PackageIcon>
    <RepositoryUrl>https://github.com/jelzGames/Barracuda</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>OpenApi,OpenApi 3.0,NET Core 3.1,Azure Functions,Azure,Swagger,Microservices,Clean Architecture,dependency injection,Serverless,Open Source,C#,CSharp,Visual Studio</PackageTags>
    <NeutralLanguage />
    <PackageReleaseNotes>Generate the json file openAPI 3.0 and the front end view to interact with the API’s calls based on the setting defined in the json file openAPI 3.0.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Templates\Auth.html" />
    <None Remove="Templates\OpenApi.html" />
    <None Include="..\..\..\Logo\barracuda 150x150 (1).png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Templates\Auth.html" />
    <EmbeddedResource Include="Templates\OpenApi.html" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

</Project>
